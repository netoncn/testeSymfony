{% extends 'base.html.twig' %}

{% block title %}Tecnicos{% endblock %}

{% block body %}
    <h1>Tecnicos</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tecnico.id }}</td>
            </tr>
            <tr>
                <th>Cpf</th>
                <td>{{ tecnico.cpf }}</td>
            </tr>
            <tr>
                <th>Nome_completo</th>
                <td>{{ tecnico.nomeCompleto }}</td>
            </tr>
            <tr>
                <th>Dt_nasc</th>
                <td>{{ tecnico.dtNasc ? tecnico.dtNasc|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Valor_hora</th>
                <td>{{ tecnico.valorHora }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('tecnicos_index') }}">back to list</a>

    <a href="{{ path('tecnicos_edit', {'id': tecnico.id}) }}">edit</a>

    {{ include('tecnicos/_delete_form.html.twig') }}
{% endblock %}
